{
  "name": "storybook-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vizzly/dashboard": "0.4.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "yarn prettier --write ./src/**",
    "deploy-storybook": "storybook-to-aws-s3 --aws-profile=NONE --ci --bucket-path=storybook.vizzly.co --s3-sync-options='--acl=public-read --delete'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/blocks": "^7.0.22",
    "@storybook/manager-api": "^7.0.22",
    "@storybook/preset-create-react-app": "^7.0.22",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^7.0.22",
    "@storybook/react-webpack5": "^7.0.22",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.22",
    "@types/react-modal": "^3.16.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^6.19.8",
    "eslint-plugin-storybook": "^0.6.12",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.22",
    "webpack": "^5.87.0"
  }
}
